/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=notc);
    And(a=notc, b=a, out=aAndNotc);
    And(a=sel, b=b, out=bAndc);
    Or(a=aAndNotc, b=bAndc, out=out);
}